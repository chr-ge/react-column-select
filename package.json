{
  "name": "react-column-select",
  "version": "1.1.1",
  "description": "React component to select items from a column list.",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "eslint": "eslint ./src/**/*.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint:css": "stylelint ./src/**/*style.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build",
    "test": "jest --env=jsdom",
    "test:cov": "yarn jest --coverage"
  },
  "homepage": "https://react-column-select.chr-ge.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chr-ge/react-column-select.git"
  },
  "bugs": {
    "url": "https://github.com/chr-ge/react-column-select/issues"
  },
  "author": "chr-ge",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest-axe": "^3.5.1",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-loader": "^8.2.2",
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-testing-library": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.5.5",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "stylelint ./src/**/*style.ts",
      "eslint ./src/**/*.tsx"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "files": [
    "dist"
  ],
  "main": "./dist/react-column-select.cjs.js",
  "module": "./dist/react-column-select.es5.js",
  "typings": "./dist/index.d.ts",
  "keywords": [
    "react",
    "react component",
    "select",
    "react select",
    "column select",
    "react column select"
  ]
}
